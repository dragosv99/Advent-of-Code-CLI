{
  "name": "aoc-cli",
  "version": "1.0.0",
  "description": "A simple and intuitive client to be used for solving and submitting solutions to AdventOfCode",
  "private": true,
  "type": "module",
  "scripts": {
    "launch:dev": "node -r esm src/index.js",
    "test": "echo \"Not tests yet\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "author": "",
  "license": "ISC",
  "bin": {
    "advent": "./src/index.js"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "figlet": "^1.5.0",
    "husky": "^4.3.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^3.0.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1"
  }
}
